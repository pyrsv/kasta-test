{
  "card": {
    "mask": ["\\d", "\\d", "\\d", "\\d", " ", "\\d", "\\d", "\\d", "\\d", " ", "\\d", "\\d",
      "\\d", "\\d", " ", "\\d", "\\d", "\\d", "\\d"],
    "regex": ["^(?:4[0-9]{12}(?:[0-9]{3})?)$", "^(?:5[1-5][0-9]{14})$"]
  },
  "expiryDate": {
    "mask": ["\\d", "\\d", "/", "\\d", "\\d"],
    "regex": ["^\\d{2}\/\\d{2}$"]
  },
  "CVV" : {
    "mask": ["\\d", "\\d", "\\d"],
    "regex": ["^\\d{3}$"]
  },
  "cardHolder": {
    "regex": ["\\D+\\s*"]
  }
}